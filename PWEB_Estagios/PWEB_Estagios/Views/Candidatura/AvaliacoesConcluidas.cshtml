@model IEnumerable<PWEB_Estagios.Models.CandidaturaProposta>

@{
    ViewBag.Title = "Estágios/Projetos Concluidos";
}

<h2>Estágios/Projetos Concluidos</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PropostaId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlunoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlunoNome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotaProposta)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PropostaId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlunoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AlunoNome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotaProposta)
        </td>
    </tr>
}

</table>
